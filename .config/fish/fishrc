# .fishrc

set -x EDITOR 'vim -f'
set -x BROWSER 'firefox'

alias sudo='sudo'
alias ls='ls --color -I LICENSE -I COPYING -I README.md'

alias xbps-i='xbps-install'
alias xbps-r='xbps-remove'
alias xbps-q='xbps-query'
alias sxbps-i='sudo xbps-install'
alias sxbps-r='sudo xbps-remove'
alias sxbps-q='sudo xbps-query'
